<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="settingEditProfileVM" type="com.digitalbanking.app.modules.settingeditprofile.data.viewmodel.SettingEditProfileVM"/>
    </data>
    <ScrollView android:clipToPadding="false"
        android:fillViewport="true"
        android:id="@+id/scrollViewScrollview"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnellipsetwentyeight"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/framePRofileImg"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_170pxv" android:layout_width="@dimen/_174pxh">
                    <ImageView android:id="@+id/imageEllipseTwentyEight"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_170pxh"
                        android:layout_width="@dimen/_170pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse28_170x170}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse28_170x170"/>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_indigo_600_radius_19"
                        android:id="@+id/btnTicket"
                        android:layout_gravity="bottom|end"
                        android:layout_height="@dimen/_38pxh"
                        android:layout_width="@dimen/_38pxh"
                        android:paddingHorizontal="@dimen/_9pxh"
                        android:paddingVertical="@dimen/_9pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_ticket_18x18" tools:ignore="ContentDescription"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtYourName"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{settingEditProfileVM.settingEditProfileModel.txtYourName}"
                    style="@style/txtInterregular13_1" tools:text="@string/lbl_your_name"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_100_05_radius_10"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:hint="@string/lbl_charlene_reed"
                    android:id="@+id/etLanguage"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={settingEditProfileVM.settingEditProfileModel.etLanguageValue}"
                    android:textColor="@color/blue_gray_900"
                    android:textColorHint="@color/blue_gray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtUserName"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{settingEditProfileVM.settingEditProfileModel.txtUserName}"
                    style="@style/txtInterregular13_1" tools:text="@string/lbl_user_name"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_100_05_radius_10"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:hint="@string/lbl_charlene_reed"
                    android:id="@+id/etLanguageOne"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={settingEditProfileVM.settingEditProfileModel.etLanguageOneValue}"
                    android:textColor="@color/blue_gray_900"
                    android:textColorHint="@color/blue_gray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmail"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{settingEditProfileVM.settingEditProfileModel.txtEmail}"
                    style="@style/txtInterregular13_1" tools:text="@string/lbl_email"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_100_05_radius_10"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:hint="@string/lbl_user_gmail_com"
                    android:id="@+id/etEmailOne"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={settingEditProfileVM.settingEditProfileModel.etEmailOneValue}"
                    android:textColor="@color/blue_gray_900"
                    android:textColorHint="@color/blue_gray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPassword"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{settingEditProfileVM.settingEditProfileModel.txtPassword}"
                    style="@style/txtInterregular13_1" tools:text="@string/lbl_password"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_100_05_radius_10"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:hint="@string/lbl"
                    android:id="@+id/etPasswordOne"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={settingEditProfileVM.settingEditProfileModel.etPasswordOneValue}"
                    android:textColor="@color/blue_gray_900"
                    android:textColorHint="@color/blue_gray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDateofBirth"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{settingEditProfileVM.settingEditProfileModel.txtDateofBirth}"
                    style="@style/txtInterregular13_1" tools:text="@string/lbl_date_of_birth"/>
                <Spinner
                    android:background="@drawable/layer_list_bg_white_a700_border_gray_100_05_radius_11"
                    android:id="@+id/spinnerDate"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPresentAddress"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{settingEditProfileVM.settingEditProfileModel.txtPresentAddress}"
                    style="@style/txtInterregular13_1" tools:text="@string/lbl_present_address"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_100_05_radius_10"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:hint="@string/msg_san_jose_calif"
                    android:id="@+id/etGroup572"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={settingEditProfileVM.settingEditProfileModel.etGroup572Value}"
                    android:textColor="@color/blue_gray_900"
                    android:textColorHint="@color/blue_gray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPermanentAddress"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{settingEditProfileVM.settingEditProfileModel.txtPermanentAddress}"
                    style="@style/txtInterregular13_1" tools:text="@string/msg_permanent_addre"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_100_05_radius_10"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:hint="@string/msg_san_jose_calif"
                    android:id="@+id/etGroup572One"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={settingEditProfileVM.settingEditProfileModel.etGroup572OneValue}"
                    android:textColor="@color/blue_gray_900"
                    android:textColorHint="@color/blue_gray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCity"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{settingEditProfileVM.settingEditProfileModel.txtCity}"
                    style="@style/txtInterregular13_1" tools:text="@string/lbl_city"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_100_05_radius_10"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:hint="@string/lbl_san_jose"
                    android:id="@+id/etGroup572Two"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={settingEditProfileVM.settingEditProfileModel.etGroup572TwoValue}"
                    android:textColor="@color/blue_gray_900"
                    android:textColorHint="@color/blue_gray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPostalCode"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{settingEditProfileVM.settingEditProfileModel.txtPostalCode}"
                    style="@style/txtInterregular13_1" tools:text="@string/lbl_postal_code"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_100_05_radius_10"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:hint="@string/lbl_45962"
                    android:id="@+id/etZipcode" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={settingEditProfileVM.settingEditProfileModel.etZipcodeValue}"
                    android:textColor="@color/blue_gray_900"
                    android:textColorHint="@color/blue_gray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCountry"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{settingEditProfileVM.settingEditProfileModel.txtCountry}"
                    style="@style/txtInterregular13_1" tools:text="@string/lbl_country"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_100_05_radius_10"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:hint="@string/lbl_usa"
                    android:id="@+id/etGroup572Three"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={settingEditProfileVM.settingEditProfileModel.etGroup572ThreeValue}"
                    android:textColor="@color/blue_gray_900"
                    android:textColorHint="@color/blue_gray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_bg_indigo_600_radius_9"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:gravity="center" android:id="@+id/btnSave"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_126pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_save"
                    android:textAllCaps="false"
                    android:textColor="@color/white_A700"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_15pxh"
                    android:textStyle="bold" tools:text="@string/lbl_save"/>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</layout>
