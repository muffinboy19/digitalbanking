<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="drawerItemSideMenuModel" type="com.digitalbanking.app.modules.itemsidemenu.data.model.DrawerItemSideMenuModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearSideMenu"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearSidebar"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_30pxv"
            android:paddingEnd="@dimen/_40pxh"
            android:paddingStart="@dimen/_40pxh"
            android:paddingTop="@dimen/_30pxv" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearLogo"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageLOGO"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_logo_indigo_600"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_logo_indigo_600"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtFirstBank"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{drawerItemSideMenuModel.txtFirstBank}"
                    style="@style/txtInterbold25" tools:text="@string/lbl_firstbank"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageHome"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom"
                    android:id="@+id/txtDashboard"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{drawerItemSideMenuModel.txtDashboard}"
                    style="@style/txtIntermedium18" tools:text="@string/lbl_dashboard"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowtransferone"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageTransferOne"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_transfer1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_transfer1"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom"
                    android:id="@+id/txtTransactions"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{drawerItemSideMenuModel.txtTransactions}"
                    style="@style/txtIntermedium18" tools:text="@string/lbl_transactions"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowuser"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageUser"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_blue_gray_400"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_blue_gray_400"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtAccounts"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{drawerItemSideMenuModel.txtAccounts}"
                    style="@style/txtIntermedium18" tools:text="@string/lbl_accounts"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowmap"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageMap"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_map"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_map"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom"
                    android:id="@+id/txtInvestments"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{drawerItemSideMenuModel.txtInvestments}"
                    style="@style/txtIntermedium18" tools:text="@string/lbl_investments"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowfile"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageFile"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_file_blue_gray_400"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_file_blue_gray_400"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom"
                    android:id="@+id/txtCreditCards"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{drawerItemSideMenuModel.txtCreditCards}"
                    style="@style/txtIntermedium18" tools:text="@string/lbl_credit_cards"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowquestion"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageQuestion"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_question_blue_gray_400"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_question_blue_gray_400"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtLoans"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{drawerItemSideMenuModel.txtLoans}"
                    style="@style/txtIntermedium18" tools:text="@string/lbl_loans"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowserviceone"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageServiceOne"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_service1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_service1"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom"
                    android:id="@+id/txtServices"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{drawerItemSideMenuModel.txtServices}"
                    style="@style/txtIntermedium18" tools:text="@string/lbl_services"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowlightbulb"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageLightbulb"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lightbulb"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lightbulb"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom"
                    android:id="@+id/txtMyPrivileges"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{drawerItemSideMenuModel.txtMyPrivileges}"
                    style="@style/txtIntermedium18" tools:text="@string/lbl_my_privileges"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowsettings"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSettings"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom"
                    android:id="@+id/txtSetting"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{drawerItemSideMenuModel.txtSetting}"
                    style="@style/txtIntermedium18" tools:text="@string/lbl_setting"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
